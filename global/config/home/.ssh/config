AddKeysToAgent yes
Compression yes

Host oronos oronospolytechnique.com
	HostName oronospolytechnique.com
	User anes.belfodil

Host rp-1
	User ubuntu
	Match host rp-1 exec "nc -z rp-1.local 22"
		HostName rp-1.local
		Port 22
	Match host rp-1
		HostName home.belfodil.ca
		Port 9518

